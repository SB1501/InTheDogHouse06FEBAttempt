--CREATE DATABASE NorthwestVetsDB;
--USE NorthwestVetsDB;

---- CUSTOMER TABLE
CREATE TABLE CustomerTbl (
    CustomerNo VARCHAR(15) CHECK (CustomerNo LIKE 'CUS-%'),
    CustomerTitle VARCHAR(10),
    CustomerName VARCHAR(50) NOT NULL,
    CustomerStreet VARCHAR(100) NOT NULL,
    CustomerTown VARCHAR(50) NOT NULL,
    CustomerCounty VARCHAR(50),
    CustomerPostcode VARCHAR(8) NOT NULL,
    CustomerPhoneNumber VARCHAR(15) UNIQUE,
    CustomerEmail VARCHAR(100) UNIQUE,
    CONSTRAINT PKCustomer PRIMARY KEY (CustomerNo)
);

-- VET TABLE
CREATE TABLE VetTbl (
    VetNo VARCHAR(15) CHECK (VetNo LIKE 'VET-%'),
    VetForename VARCHAR(50) NOT NULL,
    VetSurname VARCHAR(50) NOT NULL,
    Speciality VARCHAR(100),
    ContactNumber VARCHAR(15),
    VetEmail VARCHAR(100) UNIQUE,
    CONSTRAINT PKVet PRIMARY KEY (VetNo)
);

-- APPOINTMENT TYPE TABLE
CREATE TABLE AppointmentTypeTbl (
    AppointmentTypeID VARCHAR(15) CHECK (AppointmentTypeID LIKE 'APT-TYP-%'),
    AppointmentTypeDesc VARCHAR(50) UNIQUE NOT NULL,
    CONSTRAINT PKAppointmentType PRIMARY KEY (AppointmentTypeID)
);

-- PET TYPE TABLE
CREATE TABLE PetTypeTbl (
    PetTypeID VARCHAR(15) CHECK (PetTypeID LIKE 'PET-TYP-%'),
    PetTypeDesc VARCHAR(50) UNIQUE NOT NULL,
    CONSTRAINT PKPetType PRIMARY KEY (PetTypeID)
);

-- PET BREED TABLE
CREATE TABLE PetBreedTbl (
    BreedID VARCHAR(20) CHECK (BreedID LIKE 'BRD-%'), 
    BreedName VARCHAR(50) UNIQUE NOT NULL,
    CONSTRAINT PKPetBreed PRIMARY KEY (BreedID)
);

-- TREATMENT TYPE TABLE
CREATE TABLE TreatmentTypeTbl (
    TreatmentTypeNo VARCHAR(15) CHECK (TreatmentTypeNo LIKE 'TYP-%'),
    TreatmentTypeName VARCHAR(100) NOT NULL,
    CONSTRAINT PKTreatmentType PRIMARY KEY (TreatmentTypeNo)
);

-- TREATMENT TABLE
CREATE TABLE TreatmentTbl (
    TreatmentNo VARCHAR(15) CHECK (TreatmentNo LIKE 'TRT-%'),
    TreatmentName VARCHAR(100) NOT NULL,
    CostPrice DECIMAL(10, 2) NOT NULL,
    SellingPrice DECIMAL(10, 2) NOT NULL,
    QtyInStock INT NOT NULL,
    ExpiryDate DATE,
    TreatmentTypeNo VARCHAR(15),
    CONSTRAINT PKTreatment PRIMARY KEY (TreatmentNo),
    CONSTRAINT FKTreatmentTreatmentType FOREIGN KEY (TreatmentTypeNo) REFERENCES TreatmentTypeTbl(TreatmentTypeNo)
);

-- PET TABLE
CREATE TABLE PetTbl (
    PetNo VARCHAR(15) CHECK (PetNo LIKE 'PET-%'),
    CustomerNo VARCHAR(15) NOT NULL,
    PetName VARCHAR(50) NOT NULL,
    PetTypeID VARCHAR(15) NOT NULL,
    BreedID VARCHAR(20) NOT NULL,
    PetDOB DATE,
    PetWeight DECIMAL(5,2),
    VaccinationStatus BIT,
    MicrochipStatus BIT,
    MicrochipNumber VARCHAR(20) ,
    CONSTRAINT PKPet PRIMARY KEY (PetNo),
    CONSTRAINT FKPetCustomer FOREIGN KEY (CustomerNo) REFERENCES CustomerTbl(CustomerNo),
    CONSTRAINT FKPetType FOREIGN KEY (PetTypeID) REFERENCES PetTypeTbl(PetTypeID),
    CONSTRAINT FKPetBreed FOREIGN KEY (BreedID) REFERENCES PetBreedTbl(BreedID)
);


-- APPOINTMENT TABLE
CREATE TABLE AppointmentTbl (
    AppointmentNo VARCHAR(15) CHECK (AppointmentNo LIKE 'APT-%'),
    VetNo VARCHAR(15) NOT NULL,
    CustomerNo VARCHAR(15) NOT NULL,
    AppointmentTypeID VARCHAR(15) NOT NULL,
    AppointmentDate DATE NOT NULL,
    AppointmentTime TIME NOT NULL,
    TreatmentRoom VARCHAR(10),
    Payment BIT DEFAULT 0, -- Indicates admin status (1 = paid, 0 = Not paid)
    CONSTRAINT PKAppointment PRIMARY KEY (AppointmentNo),
    CONSTRAINT FKAppointmentVet FOREIGN KEY (VetNo) REFERENCES VetTbl(VetNo),
    CONSTRAINT FKAppointmentCustomer FOREIGN KEY (CustomerNo) REFERENCES CustomerTbl(CustomerNo),
    CONSTRAINT FKAppointmentType FOREIGN KEY (AppointmentTypeID) REFERENCES AppointmentTypeTbl(AppointmentTypeID)
);

-- APPOINTMENT DETAIL TABLE
CREATE TABLE AppointmentDetailTbl (
    AppointmentNo VARCHAR(15) NOT NULL,
    PetNo VARCHAR(15) NOT NULL,
    TreatmentNo VARCHAR(15) NOT NULL,
    Dosage VARCHAR(50),
    NoDays INT,
    TimeDaily INT,
    TreatmentStartDate DATE,
    CONSTRAINT PKAppointmentDetail PRIMARY KEY (AppointmentNo, PetNo, TreatmentNo),
    CONSTRAINT FKAppointmentDetailAppointment FOREIGN KEY (AppointmentNo) REFERENCES AppointmentTbl(AppointmentNo),
    CONSTRAINT FKAppointmentDetailPet FOREIGN KEY (PetNo) REFERENCES PetTbl(PetNo),
    CONSTRAINT FKAppointmentDetailTreatment FOREIGN KEY (TreatmentNo) REFERENCES TreatmentTbl(TreatmentNo)
);

-- SUPPLIER TABLE
CREATE TABLE SupplierTbl (
    SupplierNo VARCHAR(15) CHECK (SupplierNo LIKE 'SUP-%'),
    SupplierName VARCHAR(50) NOT NULL,
    ContactForename VARCHAR(50) NOT NULL,
    ContactSurname VARCHAR(50) NOT NULL,
    StreetName VARCHAR(100) NOT NULL,
    City VARCHAR(50) NOT NULL,
    County VARCHAR(50),
    Postcode VARCHAR(8) NOT NULL,
    PhoneNumber VARCHAR(15) NOT NULL,
    Email VARCHAR(100) NOT NULL,
    CONSTRAINT PKSupplier PRIMARY KEY (SupplierNo)
);

-- ORDER STATUS TABLE
CREATE TABLE OrderStatusTbl (
    StatusID VARCHAR(15) CHECK (StatusID LIKE 'ORD-ST-%'),
    StatusName VARCHAR(20) NOT NULL,
    CONSTRAINT PKOrderStatus PRIMARY KEY (StatusID)
);

-- ORDER TABLE
CREATE TABLE OrderTbl (
    OrderNo VARCHAR(15) CHECK (OrderNo LIKE 'ORD-%'),
    SupplierNo VARCHAR(15) NOT NULL,
    OrderDate DATE NOT NULL,
    DeliveryDate DATE,
    StatusID VARCHAR(15),
    PaymentStatus BIT DEFAULT 0,
    CONSTRAINT PKOrder PRIMARY KEY (OrderNo),
    CONSTRAINT FKOrderSupplier FOREIGN KEY (SupplierNo) REFERENCES SupplierTbl(SupplierNo),
    CONSTRAINT FKOrderStatus FOREIGN KEY (StatusID) REFERENCES OrderStatusTbl(StatusID)
);

-- ORDER DETAIL TABLE
CREATE TABLE OrderDetailTbl (
    OrderNo VARCHAR(15),
    TreatmentNo VARCHAR(15),
    QtyOrdered INT NOT NULL,
    CONSTRAINT PKOrderDetail PRIMARY KEY (OrderNo, TreatmentNo),
    CONSTRAINT FKOrderDetailOrder FOREIGN KEY (OrderNo) REFERENCES OrderTbl(OrderNo),
    CONSTRAINT FKOrderDetailTreatment FOREIGN KEY (TreatmentNo) REFERENCES TreatmentTbl(TreatmentNo)
);

-- USER TABLE
CREATE TABLE UserTbl (
    UserID INT IDENTITY(1,1), -- Auto-incrementing unique identifier
    Username VARCHAR(50) NOT NULL UNIQUE, -- Unique username
    PasswordHash VARCHAR(255) NOT NULL, -- Stores hashed passwords for security
    IsAdmin BIT NOT NULL, -- Indicates admin status (1 = Admin, 0 = Not Admin)
    CONSTRAINT PKUser PRIMARY KEY (UserID)
);


